<?xml version="1.0" encoding="UTF-8"?>

<!--  DH Vampire F3, LesterBoffo modded from StuartC @ FGUK
-->

<airplane mass="7283">

  <approach speed="110" aoa="6" fuel="0.3">
    <control-setting axis="/controls/engines/engine[0]/throttle" value="0.2"/>
    <control-setting axis="/controls/flight/flaps" value="0.25"/>
    <control-setting axis="/controls/gear/gear-down" value="1"/>
  </approach>

  <cruise speed="538" alt="20000" fuel="0.7">
    <control-setting axis="/controls/engines/engine[0]/throttle" value="1"/>
    <control-setting axis="/controls/flight/flaps" value="0"/>
    <control-setting axis="/controls/gear/gear-down" value="0"/>
  </cruise>

  <cockpit x="1.3" y="0.0" z="0.7"/>

  <fuselage ax="2.6132" ay="0.0" az="-0.11"  bx="0.8739" by="0.0" bz="0.163" width="1.24" taper="0.25" midpoint="1.0"/>
  <fuselage ax="0.8739" ay="0.0" az="0.163"  bx="-1.953" by="0.0" bz="0.011" width="1.51" taper="0.79" midpoint="0.50" cy="2.6" cz="3.6"/>
  <fuselage ax="-1.953" ay="0.0" az="0.011"  bx="-3.0604" by="0.0" bz="-0.056" width="0.81" taper="1.0" midpoint="0.0" cy="1.75" cz="1.75"/>
   <!-- tail-booms -->
 <fuselage ax="-0.4503" ay="1.4131" az="0.1056"  bx="-6.2241" by="1.4131" bz="0.3825" width="0.25" taper="0.7" midpoint="0.5" cy="1.15" cz="1.15"/>
<fuselage ax="-0.4503" ay="-1.4131" az="0.1056"  bx="-6.2241" by="-1.4131" bz="0.3825" width="0.25" taper="0.7" midpoint="0.5" cy="1.15" cz="1.15"/>

  
  <wing x="-0.823" y="0.622" z="-0.12"
        chord="3.255" 
        length="5.101"
        taper="0.23"
        incidence="1.2" 
        twist="-1"
        camber="0.04"
        sweep="-1.4" 
        dihedral="3">
        <stall aoa="16" width="5" peak="2.3"/>
        <spoiler start="0.374" end="0.613" lift="1.3" drag="1.5"/>
        <flap0 start="0.00" end="0.42" lift="1.43" drag="1.85"/>
        <flap1 start="0.47" end="0.93" lift="1.33" drag="1.12"/>

        <control-input axis="/controls/flight/spoilers" control="SPOILER"/>
        <control-input axis="/controls/flight/flaps" control="FLAP0"/> 
        <control-input axis="/controls/flight/aileron" control="FLAP1" square="true" split="true"/> 
        <control-input axis="/controls/flight/aileron-trim" control="FLAP1" split="true"/>

        <control-output control="SPOILER" prop="/surface-positions/speedbrake-pos-norm"/>  
        <control-output control="FLAP0" side="left" prop="/surface-positions/left-flap-pos-norm"/>
        <control-output control="FLAP0" side="right" prop="/surface-positions/right-flap-pos-norm"/>
        <control-output control="FLAP0" prop="/surface-positions/flap-pos-norm"/>
        <control-output control="FLAP1" side="left" prop="/surface-positions/left-aileron-pos-norm"/>
        <control-output control="FLAP1" side="right" prop="/surface-positions/right-aileron-pos-norm"/>
        <control-output control="FLAP1" prop="/surface-positions/aileron-pos-norm"/>

        <control-speed control="FLAP0" transition-time="30"/>
        <control-speed control="FLAP1" transition-time="0.1"/>
   </wing>

   <hstab x="-5.4804" y="0" z="0.843"
         chord="1.22"
         length="1.524"
         taper="1"
         sweep="0"
         effectiveness="1.37"
         camber="0.01">
         <stall aoa="18" width="3" peak="1.5"/>
         <flap0 start="0.0" end="1" lift="1.43" drag="1.15"/>

         <control-input axis="/controls/flight/elevator" square="false" control="FLAP0" />
         <control-input axis="/controls/flight/elevator-trim" control="FLAP0"/>

         <control-output control="FLAP0" prop="/surface-positions/elevator-pos-norm"/>
  </hstab>

  <vstab x="-5.587" y="1.402" z="0.604"
         chord="1.22" 
         length="1.06" 
         taper="0.28"
         sweep="32">
         <stall aoa="16" width="3" peak="2.5"/>
         <flap0 start="0" end="1.0" lift="1.48" drag="1.13"/>

         <control-input axis="/controls/flight/rudder" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"/>
  </vstab>
  
  <vstab x="-5.587" y="-1.402" z="0.604"
         chord="1.22" 
         length="1.06" 
         taper="0.28"
         sweep="32">
         <stall aoa="16" width="3" peak="2.5"/>
         <flap0 start="0" end="1.0" lift="1.48" drag="1.13"/>

         <control-input axis="/controls/flight/rudder" control="FLAP0" invert="true"/>
         <control-input axis="/controls/flight/rudder-trim" control="FLAP0" invert="true"/>

         <control-output control="FLAP0" prop="/surface-positions/rudder-pos-norm"/>
  </vstab>

  <!-- 
   De havilland goblin 3. 3350lbf
  -->
  <jet x="-0.135" y="0" z="0"
       mass="1550" 
       n1-idle="35" 
       n1-max="100" 
       n2-idle="40" 
       n2-max="100"
       egt="800" 
       tsfc="0.839"
       thrust="3350"
       exhaust-speed="1290.0"
       spool-time="5.0"  
       epr="3.3">
       <actionpt x="-2.817" y="0" z="0.0019"/>
       <control-input axis="/controls/engines/engine[0]/throttle" control="THROTTLE"/>
  </jet>
  

  <!-- Undercarriage -->
  <!-- nose -->
  <gear x="2.462" y="0" z="-1.159"
        retract-time="7"
        compression="0.230"
        spring="0.86"
        damp="0.82" >

        <control-input axis="/controls/flight/rudder" control="STEER" src0="-1.0" src1="1.0" dst0="-0.1" dst1="0.1"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-output control="EXTEND" prop="/gear/gear[0]/position-norm"/>

        <control-speed control="EXTEND" transition-time="7"/>
  </gear>

  <!-- left main -->
  <gear x="-0.807" y="1.588" z="-1.04"
        retract-time="7"
        compression="0.16"
        spring="1"
        damp="0.42" >

        <control-input axis="/controls/gear/brake-left" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-output control="EXTEND" prop="/gear/gear[1]/position-norm"/>

        <control-speed control="EXTEND" transition-time="7"/>
  </gear>

  <!-- right main -->
  <gear x="-0.812" y="-1.588" z="-1.04"
        retract-time="7"
        compression="0.16"
        spring="1"
        damp="0.42" >

        <control-input axis="/controls/gear/brake-right" control="BRAKE"/>
        <control-input axis="/controls/gear/brake-parking" control="BRAKE"/>
        <control-input axis="/controls/gear/gear-down" control="EXTEND"/>

        <control-output control="EXTEND" prop="/gear/gear[2]/position-norm"/>

        <control-speed control="EXTEND" transition-time="7"/>
  </gear>


    <!-- landing gear drag right -->
<vstab x="-0.812" y="-1.588" z="-0.49"
				 taper="0"
				 incidence="0"
				 twist="0"
				 chord="0.204"
				 sweep="0"
				 camber="0.0"
				 dihedral="0"
				 length="0.93">
		             <stall aoa="30"
				 width="18"
				 peak="5.5"/>
		<flap0 start="0.00"
					 end="1"
					 lift="-0.04"
					 drag="1.36"/>
		<control-input axis="/controls/gear/gear-down"
									 control="FLAP0"/>
		<control-output control="FLAP0"      
                                                 prop="/gear/gear[2]/position-norm"/>
	<control-speed control="FLAP0"
									 transition-time="7" />
										
	</vstab>

     <!-- landing gear drag left -->
<vstab x="-0.812" y="1.588" z="-0.49"
				 taper="0"
				 incidence="0"
				 twist="0"
				 chord="0.204"
				 sweep="0"
				 camber="0.0"
				 dihedral="0"
				 length="0.93">
		             <stall aoa="30"
				 width="18"
				 peak="5.5"/>
		<flap0 start="0.00"
					 end="1"
					 lift="-0.04"
					 drag="1.36"/>
		<control-input axis="/controls/gear/gear-down"
									 control="FLAP0"/>
		<control-output control="FLAP0"      
                                                 prop="/gear/gear[1]/position-norm"/>
	<control-speed control="FLAP0"
									 transition-time="7" />
	</vstab>

 <!-- Nose gear drag -->

   <vstab x="2.462" y="0" z="-0.309"
				 taper="0"
				 incidence="0"
				 twist="0"
				 chord="0.094"
				 sweep="0"
				 camber="0.0"
				 dihedral="0"
				 length="1.093">
		             <stall aoa="30"
				 width="18"
				 peak="5.5"/>
		<flap0 start="0.00"
					 end="1"
					 lift="0.00"
					 drag="1.16"/>
		<control-input axis="/controls/gear/gear-down"
									 control="FLAP0"/>
		<control-output control="FLAP0"      
                                                 prop="/gear/gear[0]/position-norm"/>
	<control-speed control="FLAP0"
									 transition-time="3" />
	</vstab>

  <!-- Internal tanks 
   1945 liters wings
  -->
  <tank x="-0.69" y="-1.23" z="0.136" jet="true" capacity="1550"/>
  <tank x="-0.69"  y="1.23" z="0.136" jet="true" capacity="1550"/>
  <!-- External tanks 350 liters -->
  <tank x="-0.78" y="-2.965" z="-0.30" jet="true" capacity="279"/>
  <tank x="-0.78"  y="2.965" z="-0.30" jet="true" capacity="279"/>

          <!-- drop tank weight -->
	<weight x="-0.78" y="-2.965" z="-0.30"
					size="0.5"
					mass-prop="/yasim/weights/tank-45gal-lbs[2]"/>
	<!-- port droptank - dry wt -->
	<weight x="-0.78" y="2.965" z="-0.30"
					size="0.5"
					mass-prop="/yasim/weights/tank-45gal-lbs[3]"/>
	<!-- stbd droptank - dry wt -->

  <ballast x="1.57" y="0" z="-0.8" mass="1800"/>

</airplane>

