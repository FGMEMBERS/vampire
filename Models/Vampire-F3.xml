    <property>/gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0 </dep></entry>
      <entry><ind> 1 </ind><dep> 38 </dep></entry>
    </interpolation>
    <center>
      <x-m> -2.553 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.762 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

<animation>
    <type>translate</type>
    <object-name>lower-leg</object-name>
    <property>/gear/gear[0]/compression-norm</property>
    <interpolation>
      <entry><ind> 0 </ind><dep-m> 0.0 </dep-m></entry>
      <entry><ind> 1 </ind><dep-m> 0.092 </dep-m></entry>
    </interpolation>
    <center>
      <x-m> -2.462 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.712 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  0 </y>
      <z>  1 </z>
    </axis>
  </animation>

  <!-- front wheel gear steering animation -->
  <animation>
    <name>front-steering</name>
    <object-name>leg-fulcrum</object-name>
    <object-name>front-wheel-fork</object-name>
    <object-name>lower-leg</object-name>
    <object-name>frontwheel</object-name>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>leg-fulcrum</object-name>
    <object-name>front-wheel-fork</object-name>
    <object-name>lower-leg</object-name>
    <object-name>front-wheel</object-name>
    <property>surface-positions/rudder-pos-norm</property>
    <factor> 24 </factor>
    <center>
      <x-m> -2.464 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.623 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <!-- front wheel rotation -->
  <animation>
    <type>spin</type>
    <object-name>front-wheel</object-name>
    <property>gear/gear[0]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m> -2.3566 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.9627 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>left-tyre</object-name>
    <property>gear/gear[1]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m>  0.8074 </x-m>
      <y-m> -1.585 </y-m>
      <z-m> -0.7376 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>spin</type>
    <object-name>right-tyre</object-name>
    <property>gear/gear[2]/rollspeed-ms</property>
    <factor> 30 </factor>
    <center>
      <x-m>  0.8074 </x-m>
      <y-m> -1.5919 </y-m>
      <z-m> -0.7376 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!-- Canopy -->

  <animation>
    <type>translate</type>
     <object-name>canopy-glass</object-name>
     <object-name>canopy-frame</object-name>
     <object-name>canopy1</object-name>
     <object-name>canopy-edge</object-name>
    <property>/gear/canopy/position-norm</property>
    <factor> 0.695 </factor>
      <center>
      <x-m> -0.76 </x-m>
      <y-m>  0.56 </y-m>
      <z-m>  0 </z-m>
    </center>
    <axis>
      <x>  0.81 </x>
      <y>  0 </y>
      <z>  -0.19 </z>
    </axis>
  </animation>

  <!-- Pilot invisible from internally -->

    <animation>
        <type>select</type>
        <object-name>pilot</object-name>
		<condition>
<not>
            <property>sim/current-view/internal</property>
</not>
        </condition>
    </animation>
  
  
</PropertyList>
