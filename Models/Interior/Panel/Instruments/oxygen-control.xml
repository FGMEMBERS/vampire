<?xml version="1.0"?>
<PropertyList>
	<path>oxygen-supply.ac</path>

	<animation>
		<type>material</type>
		<object-name>Colour-Group-1</object-name>
		<ambient>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</ambient>
		<diffuse>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</diffuse>
		<specular>
			<red>1.0</red>
			<green>1.0</green>
			<blue>1.0</blue>
		</specular>
		<shininess>10</shininess>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>Colour-Group-2</object-name>
		<ambient>
			<red>0.4</red>
			<green>0.4</green>
			<blue>0.4</blue>
		</ambient>
		<diffuse>
			<red>0.4</red>
			<green>0.4</green>
			<blue>0.4</blue>
		</diffuse>
		<specular>
			<red>0.7</red>
			<green>0.7</green>
			<blue>0.7</blue>
		</specular>
		<shininess>100</shininess>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>

	<animation>
		<type>material</type>
		<object-name>Colour-Group-3</object-name>
		<ambient>
			<red>0.7</red>
			<green>0.7</green>
			<blue>0.7</blue>
		</ambient>
		<diffuse>
			<red>0.7</red>
			<green>0.7</green>
			<blue>0.7</blue>
		</diffuse>
		<specular>
			<red>0.9</red>
			<green>0.9</green>
			<blue>0.9</blue>
		</specular>
		<shininess>100</shininess>
		<emission>
			<factor-prop>/controls/lighting/panel-norm</factor-prop>
			<red>0.75</red>
			<green>0.25</green>
			<blue>0.25</blue>
		</emission>
	</animation>
	
	<animation>
		<type>pick</type>
		<object-name>pick-down</object-name>
		<visible>false</visible>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/controls/oxygen/altitude-norm</property>
				<step>-0.001</step>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/controls/oxygen/altitude-norm</property>
				<step>-0.01</step>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>

	<animation>
		<type>pick</type>
		<object-name>pick-up</object-name>
		<visible>false</visible>
		<action>
			<button>0</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/controls/oxygen/altitude-norm</property>
				<step>0.001</step>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
			<button>1</button>
			<repeatable>true</repeatable>
			<binding>
				<command>property-adjust</command>
				<property>/controls/oxygen/altitude-norm</property>
				<step>0.01</step>
				<min>0</min>
				<max>1</max>
				<wrap>false</wrap>
			</binding>
		</action>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>Handle</object-name>
		<property>/controls/oxygen/altitude-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>1080</dep>
			</entry>
		</interpolation>
		<!--0.0198707	-0.019419	4.2608e-008-->
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.019419</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle-alt</object-name>
		<property>/controls/oxygen/altitude-norm</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-150</dep>
			</entry>
			<entry>
				<ind>0.5</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>1</ind>
				<dep>150</dep>
			</entry>
		</interpolation>
		<!--0.0035198	-2.30837e-006	0.0309151
	0.00363644	-2.30837e-006	0.0309151-->
		<center>
			<x-m>0.0035198</x-m>
			<y-m>-0.0309151</y-m>
			<z-m></z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>needle-contents</object-name>
		<property>consumables/oxygen/tank/pressure-psi</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>-150</dep>
			</entry>
			<entry>
				<ind>1000</ind>
				<dep>0</dep>
			</entry>
			<entry>
				<ind>2000</ind>
				<dep>150</dep>
			</entry>
		</interpolation>
		<!--0.0035198	-2.30837e-006	0.0309151
	0.00363644	-2.30837e-006	0.0309151-->
		<center>
			<x-m>0.0035198</x-m>
			<y-m>0.0309151</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

